#ifndef STATES_H_
#define STATES_H_

#include "BareMetalDriver.h"

#define Define_State(state) void ST_##state(__VA_ARGS__)
#define State(state) ST_##state

typedef enum {
	Green_On,
	Green_Off,
	Yellow_On,
	Yellow_Off,
	Red_On,
	Red_Off
}Indicator_status;

#endif /* STATES_H_ */
